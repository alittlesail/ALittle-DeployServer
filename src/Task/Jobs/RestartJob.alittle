
namespace DeployServer;

protected class RestartJob : Job
{
    public await fun Execute(BuildInfo build_info) : string, string
    {
        var loop = new ALittle.LoopTimer(bind(__CPPAPI_ServerSchedule.RestartAll, __CPPAPI_ServerSchedule), 2000);
        loop.Start();
        return null, null;
    }
}
